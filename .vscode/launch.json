    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "env": {
                "WANDB_MODE": "offline",
                "CUDA_VISIBLE_DEVICES":"0",
            },
            "justMyCode": false,
            "args": [
                "--config",
                "/root/code/trl/orng_conf/grpo_bias_debug.yaml",
            ]
        }
    ]
}